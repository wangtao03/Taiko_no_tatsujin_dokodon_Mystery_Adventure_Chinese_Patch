Msg_Visible(true)
Msg_Load("SUB_409_000")
System_LoadWait()
if StorySystem_GetStory() >= STORY_007_G_R then
  if StorySystem_GetStory() < STORY_008_A then
    Msg_SetLabel("SUB_409_000_Ms_300", true, true)
end
else
  if StorySystem_GetStory() >= STORY_008_G then
    Msg_SetLabel("SUB_409_000_Ms_300", true, true)
  else
    Msg_SetLabel("SUB_409_000_Ms_000", true, true)
  end
end
Msg_Choise("是\n否")
if Msg_GetChoise() == 2 then
  Msg_SetLabel("SUB_409_000_Ms_200", true, true)
  Msg_Clear()
  Msg_Visible(false)
  return 0
end
if Msg_GetChoise() == 1 then
  Se_Load(WSDSET_GIMMICK)
  System_LoadWait()
  if StorySystem_GetStory() >= STORY_007_G_R then
    if StorySystem_GetStory() < STORY_008_A then
      Msg_SetLabel("SUB_409_000_Ms_400", true, true)
  end
  else
    if StorySystem_GetStory() >= STORY_008_G then
      Msg_SetLabel("SUB_409_000_Ms_400", true, true)
    else
      Msg_SetLabel("SUB_409_000_Ms_100", true, true)
    end
  end
  Msg_Clear()
  Msg_Visible(false)
  System_WaitFrame(5)
  Se_Play(ADV_SE_PLAY_00, WSD_SE_GM_WARP)
  Display_FadePlay(FADE_TYPE_OUT, FADE_COLOR_WHITE, FADE_FRAME_SLOW, FADE_DISPLAY_UD)
  Display_FadeWait()
  Field_MapChange(MAP_CHANGE_DUNGEON, 7, 8, "_DEFAULT_000")
  return 0
end
